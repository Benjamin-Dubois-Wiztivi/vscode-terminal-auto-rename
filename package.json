{
  "name": "terminal-auto-rename",
	"displayName": "Terminal Auto Rename",
	"description": "Automatically renames the terminal with the current folder name.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:terminal-auto-rename.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "terminal-auto-rename.helloWorld",
            "title": "Terminal Auto Rename"
		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.54.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2"
    }

}
